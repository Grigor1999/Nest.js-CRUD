{
  "name": "user-service",
  "version": "2.0.0",
  "description": "user service",
  "license": "Proprietary",
  "scripts": {
    "start": "LOG_LEVEL=info node build/src/index.js",
    "start:staging": "NODE_ENV=staging npm run start",
    "start:production": "NODE_ENV=production npm run start",
    "test": "NODE_ENV=testing jest -c .jest-unit.json --watchAll",
    "test:debug": "NODE_ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c .jest-unit.json --runInBand",
    "test:once": "NODE_ENV=testing jest -c .jest-unit.json --coverage",
    "posttest:once": "npm run check",
    "e2e": "NODE_ENV=e2e jest -c .jest-e2e.json --watchAll",
    "e2e:debug": "NODE_ENV=e2e node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c .jest-e2e.json --runInBand",
    "e2e:once": "NODE_ENV=e2e jest -c .jest-e2e.json",
    "dev": "npm run clean && NODE_ENV=development LOG_LEVEL=debug tsc-watch --preserveWatchOutput -w --noEmitOnError false --onSuccess \"node -r source-map-support/register ./build/src/index.js\"",
    "dev:debug": "npm run clean && NODE_ENV=development LOG_LEVEL=debug tsc-watch --preserveWatchOutput -w --noEmitOnError false --onSuccess \"node -r source-map-support/register --inspect-brk=0.0.0.0:9222 ./build/src/index.js\"",
    "clean": "rm -rf build/",
    "compile": "echo Compiling... && tsc -p .",
    "compile:staging": "npm run compile:production",
    "compile:production": "npm run clean && echo Compiling... && tsc -p tsconfig.build.json",
    "check": "eslint -c .eslintrc.fix.json '{src,test}/**/*.{ts,js}'",
    "fix": "eslint -c .eslintrc.fix.json --fix '{src,test}/**/*.{ts,js}'",
    "shell:build": "docker-compose -f docker-compose.shell.yml build",
    "shell": "npm run shell:build && docker-compose  -f docker-compose.shell.yml run --rm -u 1000 --name=typescript-starter-CHANGEME --service-ports app bash || true && echo Stopping environment... && npm run shell:stop",
    "shell:join": "docker  exec -u 0 -ti typescript-starter-CHANGEME bash",
    "shell:stop": "docker-compose -f docker-compose.shell.yml down",
    "precommit": "lint-staged",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -d ./src/migrations -n $1",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -d ./src/migrations -n $1",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@elunic/ecs-commitlint": "^0.1.0",
    "@elunic/ecs-prettier": "^0.2.1",
    "@elunic/ecs-tsconfig": "^0.1.0",
    "@elunic/eslint-config-ecs": "^0.1.2",
    "@nestjs/testing": "^7.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.8.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/mysql": "^2.15.14",
    "@types/node": "^12.12.47",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/prettier": "^1.19.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon": "^7.5.2",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^3.4.9",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-extended": "^0.11.5",
    "jest-html-reporters": "^2.1.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^8.1.0",
    "moment": "^2.27.0",
    "prettier": "^2.0.5",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.9.5"
  },
  "dependencies": {
    "@elunic/is-iso-date": "^1.0.5",
    "@elunic/logger": "^4.0.3",
    "@elunic/logger-nestjs": "^1.1.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.2.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^1.1.0",
    "uuid": "^7.0.3"
  },
  "_moduleAliases": {
    "src": "build/src/"
  }
}
